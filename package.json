{
  "name": "Nitro-Sync",
  "version": "2.1.0",
  "scripts": {
    "test": "DISABLE_LOG=1 mocha -R spec --compilers coffee:coffee-script/register test/*",
    "coveralls": "DISABLE_LOG=1 ./node_modules/.bin/mocha --compilers coffee:coffee-script/register -r blanket -R mocha-lcov-reporter | ./node_modules/.bin/coveralls"
  },
  "repository": "https://github.com/CaffeinatedCode/nitro-server.git",
  "dependencies": {
    "coffee-script": "1.7.x",
    "express": "3.4.x",
    "bcrypt": "0.7.x",
    "nodemailer": "0.6.x",
    "kew": "0.3.x",
    "sockjs": "0.3.x",
    "cors": "2.1.x",
    "jandal": "0.0.x",
    "xtype": "0.0.7",
    "knex": "0.5.x",
    "mysql": "2.0.x",
    "pg": "2.11.x",
    "knex-mssql": "0.0.x",
    "swig-email-templates": "1.0.x",
    "newrelic": "1.3.x"
  },
  "devDependencies": {
    "supertest": "0.9.x",
    "mocha": "1.17.x",
    "should": "3.1.x",
    "blanket": "1.1.x",
    "forever": "0.10.x",
    "mocha-lcov-reporter": "0.0.x",
    "coveralls": "2.7.x"
  },
  "config": {
    "blanket": {
      "pattern": "app/",
      "loader": "./node-loaders/coffee-script"
    }
  },
  "engines": {
    "node": "0.10.x"
  }
}
